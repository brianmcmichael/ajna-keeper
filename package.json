    "@uniswap/sdk-core": "^7.5.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-sdk": "^3.24.0",
    "axios": "^1.8.1",
    "ethers": "^5.7.2",
    "graphql": "^16.10.0",
    "graphql-request": "6.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.1.6",
