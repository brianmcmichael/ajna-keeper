    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.10",
    "@types/sinon": "^17.0.3",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.1",
    "dotenv": "^16.4.7",
